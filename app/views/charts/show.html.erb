<h1 class="text-center">View Patient Chart</h1>
<%= render 'layouts/identifier' %>

<%= render @chart %>
<br>
<%= link_to 'Edit', edit_regi_chart_path(@regi, @chart), class: 'btn btn-warning btn-sm' %>
<%= link_to 'Charts', regi_charts_path(@regi), class: 'btn btn-success btn-sm' %>
<%= button_to "Delete", [@regi,@chart], method: :delete, class: "btn btn-danger btn-sm mt-3", from: { data: { turbo_confirm: "Are you sure?" } } %>

